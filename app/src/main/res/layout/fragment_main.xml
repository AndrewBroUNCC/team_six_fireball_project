<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ContentViewMainFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:elevation="1.5dp"
    tools:context=".MainFragment">

    <androidx.cardview.widget.CardView
        android:id="@+id/home_login_btn_container2"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:backgroundTint="@color/grey"
        android:clickable="true"
        android:foreground="?android:attr/selectableItemBackground"
        android:innerRadius="0dp"
        android:shape="rectangle"
        android:thicknessRatio="1.9"
        android:visibility="invisible"
        app:cardCornerRadius="20dp"
        app:cardElevation="1.5dp"
        app:cardMaxElevation="5dp"
        app:layout_constraintBottom_toTopOf="@+id/viewLineCardViewTop"
        app:layout_constraintEnd_toEndOf="@+id/home_logout_btn_container2"
        app:layout_constraintStart_toStartOf="@+id/home_forum_btn_container"
        app:layout_constraintTop_toBottomOf="@+id/home_username_txt"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageViewLogOutIcon"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="38dp"
            android:scaleX="1.5"
            android:scaleY="1.5"
            android:src="@drawable/ic_login_blue" />

        <TextView
            android:id="@+id/textViewHomeFragLogout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="120dp"
            android:text="Login or Register"
            android:textColor="@color/black"
            android:textSize="24sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/texthome_logout_btn_container2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="130dp"
            android:layout_marginTop="60dp"
            android:layout_marginRight="8dp"
            android:gravity="left"
            android:text="Join Our Community"
            android:textColor="@color/black"
            android:textSize="16sp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/home_geninfo_btn_container"
        android:layout_width="0dp"
        android:layout_height="80dp"
        android:backgroundTint="@color/white"
        android:clickable="true"
        android:elevation="12dp"
        android:foreground="?android:attr/selectableItemBackground"
        android:innerRadius="0dp"
        android:shape="rectangle"
        android:thicknessRatio="1.9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_graph_btn_container"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageViewGeneralInfoIcon"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="20dp"
            android:src="@drawable/ic_info_blue" />

        <View
            android:id="@+id/viewLineCardViewMidTwo"
            android:layout_width="300dp"
            android:layout_height="1dp"
            android:layout_gravity="end"
            android:layout_marginTop="2dp"
            android:background="#000000" />

        <TextView
            android:id="@+id/texthome_forum_btn_container7"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="100dp"
            android:gravity="center_vertical"
            android:text="General Information "
            android:textColor="@color/black"
            android:textSize="20sp"
            android:textStyle="bold" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/home_graph_btn_container"
        android:layout_width="0dp"
        android:layout_height="80dp"
        android:backgroundTint="@color/white"
        android:clickable="true"
        android:elevation="12dp"
        android:foreground="?android:attr/selectableItemBackground"
        android:innerRadius="0dp"
        android:shape="rectangle"
        android:thicknessRatio="1.9"
        app:layout_constraintBottom_toTopOf="@+id/home_geninfo_btn_container"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_map_btn_container"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageViewGraphIcon"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="20dp"
            android:src="@drawable/ic_graph_blue" />

        <View
            android:id="@+id/viewLineCardViewMidOne"
            android:layout_width="300dp"
            android:layout_height="1dp"
            android:layout_gravity="end"
            android:layout_marginTop="2dp"
            android:background="#000000" />

        <TextView
            android:id="@+id/texthome_forum_btn_container6"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="100dp"
            android:gravity="center_vertical"
            android:text="Visual Graphs"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:textStyle="bold" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/home_map_btn_container"
        android:layout_width="0dp"
        android:layout_height="80dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="#FFFFFF"
        android:clickable="true"
        android:elevation="12dp"
        android:foreground="?android:attr/selectableItemBackground"
        android:innerRadius="0dp"
        android:shape="rectangle"
        android:thicknessRatio="1.9"
        app:layout_constraintBottom_toTopOf="@+id/home_graph_btn_container"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_forum_btn_container"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageViewMapIcon"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="20dp"
            android:src="@drawable/ic_map_blue" />

        <TextView
            android:id="@+id/texthome_forum_btn_container5"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="100dp"
            android:gravity="center"
            android:text="FireBall Interactive Map"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:textStyle="bold" />
    </androidx.cardview.widget.CardView>

    <View
        android:id="@+id/viewLine"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_marginTop="154dp"
        android:background="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/viewLineCardViewTop"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="#000000"
        app:layout_constraintBottom_toTopOf="@+id/home_map_btn_container"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/viewLineCardViewBottom"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_marginTop="1dp"
        android:background="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_geninfo_btn_container" />

    <TextView
        android:id="@+id/textViewTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:fontFamily="@font/playfai_display_medium"
        android:text="FireBorn"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="50sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/viewLine"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/viewLine2"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_marginBottom="24dp"
        android:background="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/home_forum_btn_container"
        android:layout_width="120dp"
        android:layout_height="140dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/grey"
        android:clickable="true"
        android:foreground="?android:attr/selectableItemBackground"
        android:innerRadius="0dp"
        android:shape="rectangle"
        android:thicknessRatio="1.9"
        android:visibility="visible"
        app:cardCornerRadius="20dp"
        app:cardElevation="1.5dp"
        app:cardMaxElevation="5dp"
        app:layout_constraintEnd_toStartOf="@+id/home_profile_btn_container"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_username_txt"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageViewForumIcon"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="12dp"
            android:src="@drawable/ic_chat_blue" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="62dp"
            android:text="Forum"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/texthome_forum_btn_container2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="90dp"
            android:layout_marginRight="8dp"
            android:gravity="left"
            android:text="Post or Join a Topic"
            android:textColor="@color/black"
            android:textSize="16sp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/home_profile_btn_container"
        android:layout_width="120dp"
        android:layout_height="140dp"
        android:backgroundTint="@color/grey"
        android:clickable="true"
        android:foreground="?android:attr/selectableItemBackground"
        android:innerRadius="0dp"
        android:shape="rectangle"
        android:thicknessRatio="1.9"
        android:visibility="visible"
        app:cardBackgroundColor="#000000"
        app:cardCornerRadius="20dp"
        app:cardElevation="1.5dp"
        app:cardMaxElevation="5dp"
        app:layout_constraintEnd_toStartOf="@+id/home_logout_btn_container2"
        app:layout_constraintStart_toEndOf="@+id/home_forum_btn_container"
        app:layout_constraintTop_toTopOf="@+id/home_forum_btn_container"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageViewProfileIcon"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="12dp"
            android:src="@drawable/icon_person_blue" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="62dp"
            android:text="Profile"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/texthome_forum_btn_container3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="90dp"
            android:layout_marginRight="8dp"
            android:gravity="left"
            android:text="A Personal Profile"
            android:textColor="@color/black"
            android:textSize="16sp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/home_logout_btn_container2"
        android:layout_width="120dp"
        android:layout_height="140dp"
        android:backgroundTint="@color/grey"
        android:clickable="true"
        android:elevation="12dp"
        android:foreground="?android:attr/selectableItemBackground"
        android:innerRadius="0dp"
        android:shape="rectangle"
        android:thicknessRatio="1.9"
        android:visibility="visible"
        app:cardCornerRadius="20dp"
        app:cardElevation="1.5dp"
        app:cardMaxElevation="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/home_profile_btn_container"
        app:layout_constraintTop_toTopOf="@+id/home_forum_btn_container"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageViewLoginIcon"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="12dp"
            android:src="@drawable/ic_logout_blue" />

        <TextView
            android:id="@+id/home_log_title_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="62dp"
            android:text="Logout"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/home_log_label_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="90dp"
            android:layout_marginRight="8dp"
            android:gravity="start"
            android:text="Sign-out of Account"
            android:textColor="@color/black"
            android:textSize="16sp" />
    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="303dp"
        android:layout_height="0dp"
        android:layout_marginTop="34dp"
        android:layout_marginBottom="8dp"
        android:contentDescription="@string/website_homepage_title"
        android:scaleX="1.0"
        android:scaleY="1.0"
        android:src="@drawable/fire_born_blue_solo"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/viewLine"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewMainFragContactLink"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="~ Contact Us ~"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/viewLine2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/home_username_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:maxLines="1"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/home_forum_btn_container"
        app:layout_constraintStart_toStartOf="@+id/home_forum_btn_container"
        app:layout_constraintTop_toBottomOf="@+id/viewLine" />

</androidx.constraintlayout.widget.ConstraintLayout>